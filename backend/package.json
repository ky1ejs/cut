{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node -r dotenv/config src/index.ts",
    "dev:watch": "nodemon",
    "dev:watch:no-worker": "OFFLINE=true nodemon",
    "build": "tsc -p tsconfig.build.json && cp -r src/resources ./dist",
    "start": "node dist/index.js",
    "codegen:graphql": "gql-gen --config graphql-codegen.ts",
    "codegen:prisma": "prisma generate",
    "migrate": "prisma migrate deploy",
    "migrate:start": "pnpm run migrate && pnpm run start",
    "seed": "ts-node -r dotenv/config tests/seed.ts",
    "jest": "export $(cat tests/.env.test | xargs) && prisma migrate deploy && jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@prisma/client": "5.22.0",
    "@uploadcare/rest-client": "^6.14.1",
    "@uploadcare/signed-uploads": "^6.14.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "express": "^4.18.2",
    "google-libphonenumber": "^3.2.34",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "prisma": "^5.9.1",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.0",
    "@types/node": "^24.0.3",
    "dotenv": "^16.4.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}